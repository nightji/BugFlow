<body>
	<div id="welcome" class="setting-title" width="100%" height="100%"
		hAlign="center" backgroundImage="/images/bg.jpg" layout="VBox">
		<div width="100%" height="180" layout="HBox">
			<div width="100%" height="100%"></div>
			<div width="60" height="60" paddingLeft="10" paddingBottom="10"
				paddingTop="10" paddingRight="10">
				<img src="/images/setting.png" width="100%" height="100%"
					onClick="onClickSetting" />
			</div>
		</div>
		<div width="120" height="60" wAlign="center">
			<img src="/images/button1.png" width="100%" height="100%"
				onClick="onClikNewFlow" />
		</div>
		<div width="100%" height="100"></div>
		<div width="120" height="60" wAlign="center">
			<img src="/images/button2.png" width="100%" height="100%"
				onClick="onClickExistFlow" />
		</div>

	</div>
</body>

<script>
	var userid;
	$M.page.addEvent("onLoad", function(data) {
		userid = data.userid;
	});

	function onClickSetting() {
		var ajax = new $M.Ajax();
		ajax.add("userid", userid);
		log("00000", userid);

		ajax.setAction("/queryPro.jsp");
		ajax.submit(function(result) {
		
			$M.page.goTo({
				'url' : '/flowList.html',
				'params' : {
					jsonString:result
				},
			});
		}, function(errorCode, errorMsg) {

		});

	}

	function onClikNewFlow() {
		//todo
		$M.page.goTo({
			'url' : '/newFlow.html',
		});
	}

	function onClickExistFlow() {
		//todo
		$M.page.goTo({
			'url' : '/flowList.html',
		});
	}
</script>

</html>