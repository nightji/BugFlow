<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
</head>
<body>
	<!-- 标题栏 -->
	<div id="nav" width="100%" height="52">
		<div width="100%" height="100%" vAlign="middle" hAlign="center"
			backgroundColor="#474747">
			<input type="label" value="流程列表" color="#ffffff" fontSize="25" />
		</div>
	</div>

	<div width="100%" height="100%" layout="VBox" backgroundColor="#eaeaea">
		<listview id="listview" width="100%" height="100%"
			dividerVisible="true" dividerColor="#eaeaea"
			backgroundColor="#eaeaea"> <template>

		<div width="100%" height="-2" layout="VBox" vAlign="middle">
			<div width="100%" height="-2" marginLeft="10" marginRight="10"
				layout="VBox">
				<div width="100%" height="100%" layout="HBox">
					<input type="label" value="bug号：" fontSize="14" color="#919da4" />
					<input type="label" tag="proId" color="#919da4" fontSize="14" />

				</div>
				<input type="label" tag="proTitle" fontSize="16" fontBold="true"
					marginTop="3" />
			</div>
			<div width="100%" height="100%" layout="HBox" marginTop="3"
				marginLeft="10" marginRight="10">
				<div width="100%" height="-2" hAlign="left" layout="HBox">
					<input type="label" value="最后参与：" color="#919da4" fontSize="14" />
					<input type="label" tag="personDealNow" color="#919da4" fontSize="14" />
				</div>
				<div width="100%" height="-2" hAlign="right">
					<input type="label" tag="status" color="#919da4" fontSize="14" />
				</div>
			</div>
			<div width="100%" height="2" backgroundColor="#f4f4f4"></div>


		</div>
		</template> </listview>


	</div>
</body>
<script>
	$M.page.addEvent("onLoad", function(data) {
		var items = data.jsonString;
		alert(items);

		listview.setItems(items);
		listview.reloadData();

	});
	listview.addEvent('onItemClick', function(row, tag, data) {
		$M.page.goTo({
			'url' : '/existFlow5.html',
		});
	});
</script>
</html>
